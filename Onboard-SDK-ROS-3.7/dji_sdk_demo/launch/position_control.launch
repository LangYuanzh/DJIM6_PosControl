<launch>
    <node pkg="dji_sdk_demo" type="position_control" name="position_control" output="screen">
    <!-- node parameters -->
    <param name="alt_P1" type="double" value= "1.3" />
    <param name="vz_P" type="double" value=  "0.5"/>
    <param name="vz_I" type="double" value=  "0.05"/>
    <param name="vz_D" type="double" value=  "0.2"/>
  
    <param name="xy_P" type="double" value=  "1.2"/>
    <param name="vxy_P" type="double" value= "0.85"/>
    <param name="vxy_I" type="double" value= "0.45"/>
    <param name="vxy_D" type="double" value= "0.11"/>

    <param name="max_pitch_roll" type="double" value= "30.0"/>
    </node>
</launch>
